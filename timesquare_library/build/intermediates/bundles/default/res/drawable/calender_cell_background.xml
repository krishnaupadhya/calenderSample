<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    >
    <item app:tsquare_state_range_middle="true">
        <shape>
            <corners android:bottomLeftRadius="10dp" android:bottomRightRadius="10dp" android:topLeftRadius="5dp" android:topRightRadius="5dp" />
            <solid android:color="@color/calendar_selected_range_bg" />
            <stroke android:width="1dp" android:color="@color/calendar_selected_range_bg"></stroke>
        </shape>
    </item>
    <item android:state_selected="true">
        <shape>
            <corners android:bottomLeftRadius="10dp" android:bottomRightRadius="10dp" android:topLeftRadius="5dp" android:topRightRadius="5dp" />
            <solid android:color="@color/calendar_selected_day_bg" />
            <stroke android:width="1dp" android:color="@color/calendar_selected_day_bg"></stroke>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <corners android:bottomLeftRadius="10dp" android:bottomRightRadius="10dp" android:topLeftRadius="5dp" android:topRightRadius="5dp" />
            <solid android:color="@color/calendar_selected_day_bg" />
            <stroke android:width="1dp" android:color="@color/calendar_selected_day_bg"></stroke>
        </shape>
    </item>
    <item app:tsquare_state_current_month="false">
        <shape>
            <corners android:bottomLeftRadius="10dp" android:bottomRightRadius="10dp" android:topLeftRadius="5dp" android:topRightRadius="5dp" />
            <solid android:color="@color/creamy_white" />
            <stroke android:width="1dp" android:color="@color/creamy_white"></stroke>
        </shape>
    </item>
    <item app:tsquare_state_highlighted="true">
        <shape>
            <corners android:bottomLeftRadius="10dp" android:bottomRightRadius="10dp" android:topLeftRadius="5dp" android:topRightRadius="5dp" />
            <solid android:color="@color/calendar_highlighted_day_bg" />
            <stroke android:width="1dp" android:color="@color/calendar_highlighted_day_bg"></stroke>
        </shape>
    </item>
    <item app:tsquare_state_today="true">
        <shape>
            <corners android:bottomLeftRadius="10dp" android:bottomRightRadius="10dp" android:topLeftRadius="5dp" android:topRightRadius="5dp" />
            <solid android:color="@color/calendar_bg" />
            <stroke android:width="1dp" android:color="@color/calendar_bg"></stroke>
        </shape>
    </item>
    <item>
        <shape>
            <corners android:bottomLeftRadius="10dp" android:bottomRightRadius="10dp" android:topLeftRadius="5dp" android:topRightRadius="5dp" />
            <solid android:color="@color/calendar_bg" />
            <stroke android:width="1dp" android:color="@color/calendar_bg"></stroke>
        </shape>
    </item>
</selector>
